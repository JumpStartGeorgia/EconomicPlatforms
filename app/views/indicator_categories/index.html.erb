<h1>Listing indicator_categories</h1>

<table>
  <tr>
    <th>Name</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @indicator_categories.each do |indicator_category| %>
  <tr>
    <td><%= indicator_category.name %></td>
    <td><%= link_to 'Show', indicator_category %></td>
    <td><%= link_to 'Edit', edit_indicator_category_path(indicator_category) %></td>
    <td><%= link_to 'Destroy', indicator_category, confirm: 'Are you sure?', method: :delete %></td>
  </tr>
<% end %>
</table>

